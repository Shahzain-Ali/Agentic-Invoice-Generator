{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "100mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/main.py"
    }
  ],
  "installCommand": "uv sync --frozen --no-cache",
  "buildCommand": "echo 'Build skipped'"
}